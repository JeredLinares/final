

<h4> All Players </h4>


<table class="table">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Team</th>
    </tr>
  </thead>
  <tbody>
    <% for player in @players %>
        <tr>
        <td><%= player[:username] %></td>
        <td><%= player[:team] %></td>
        </tr>
    <% end %>
  </tbody>
</table>